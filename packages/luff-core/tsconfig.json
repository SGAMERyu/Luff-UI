{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "jsx": "preserve",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "lib": ["ESNext", "DOM"],
    "skipLibCheck": true,
    "noEmit": true,
    "types": ["unplugin-vue-macros/macros-global" /* ... */],
    "paths": {
      "~/*": ["./*"]
    }
  },
  "include": ["**/*.ts", "**/*.d.ts", "**/*.tsx", "**/*.vue", "**/src/*.vue"],
  "references": [{ "path": "./tsconfig.node.json" }],
  "vueCompilerOptions": {
    "plugins": ["./node_modules/pinceau/volar.cjs"]
  }
}
